# SPECTRUM KERNEL MANAGER
# Ramdisk file for profile based kernel management
# Implimentation inspired by Franco's fku profiles

# Initialization
on property:sys.boot_completed=1
   # Set default profile on first boot
   exec u:r:init:s0 root root -- /init.spectrum.sh
   exec u:r:su:s0 root root -- /init.spectrum.sh
   # Enable Spectrum support
   setprop spectrum.support 1

# Balance (default profile)
on property:persist.spectrum.profile=0
write /sys/devices/system/cpu/cpu1/online 1
write /sys/devices/system/cpu/cpu2/online 1
write /sys/devices/system/cpu/cpu3/online 1
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor nebula
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor nebula
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor nebula
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor nebula
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1478400
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 1478400
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2073600
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 2073600
write /sys/class/kgsl/kgsl-3dO/devfreq/min_freq 133000
write /sys/class/kgsl/kgsl-3dO/devfreq/max_freq 560000
# write /sys/kernel/gpu/gpu_min_clock 133000
# write /sys/kernel/gpu/gpu_max_clock 560000

# Performance (based on DrunkSauce Lite)
on property:persist.spectrum.profile=1
write /sys/devices/system/cpu/cpu1/online 1
write /sys/devices/system/cpu/cpu2/online 1
write /sys/devices/system/cpu/cpu3/online 1
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor interactive_pro
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor interactive_pro
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor interactive_pro
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor interactive_pro
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1728000
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 1728000
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2265600
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 2265600
write /sys/class/kgsl/kgsl-3dO/devfreq/min_freq 315000
write /sys/class/kgsl/kgsl-3dO/devfreq/max_freq 624000
# write /sys/kernel/gpu/gpu_min_clock 315000
# write /sys/kernel/gpu/gpu_max_clock 624000

# Battery
on property:persist.spectrum.profile=2
write /sys/devices/system/cpu/cpu1/online 1
write /sys/devices/system/cpu/cpu2/online 1
write /sys/devices/system/cpu/cpu3/online 0
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor zzmoove
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor zzmoove
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor zzmoove
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor zzmoove
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq 307200
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1401600
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 1401600
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 1401600
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 1401600
write /sys/class/kgsl/kgsl-3dO/devfreq/min_freq 133000
write /sys/class/kgsl/kgsl-3dO/devfreq/max_freq 315000

# write /sys/kernel/gpu/gpu_min_clock 133000
# write /sys/kernel/gpu/gpu_max_clock 315000

# Gaming
on property:persist.spectrum.profile=3
write /sys/devices/system/cpu/cpu1/online 1
write /sys/devices/system/cpu/cpu2/online 1
write /sys/devices/system/cpu/cpu3/online 1
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor performance
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_governor performance
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_governor performance
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_governor performance
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 1478400
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_min_freq 1478400
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_min_freq 2073600
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_min_freq 2073600
write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1728000
write /sys/devices/system/cpu/cpu1/cpufreq/scaling_max_freq 1728000
write /sys/devices/system/cpu/cpu2/cpufreq/scaling_max_freq 2265600
write /sys/devices/system/cpu/cpu3/cpufreq/scaling_max_freq 2265600
write /sys/class/kgsl/kgsl-3d0/devfreq/governor msm-adreno-tz
write /sys/class/kgsl/kgsl-3dO/devfreq/min_freq 560000
write /sys/class/kgsl/kgsl-3dO/devfreq/max_freq 652000
# write /sys/kernel/gpu/gpu_min_clock 560000
# write /sys/kernel/gpu/gpu_max_clock 652000

